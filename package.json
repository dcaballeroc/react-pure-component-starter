{
  "name": "react-transform-boilerplate",
  "version": "2.2.0",
  "description": "A new Webpack boilerplate with hot reloading React components, and error handling on module and component level.",
  "scripts": {
    "clean": "rimraf build",
    "build:rollup": "rollup -c",
    "build": "yarn run clean && yarn run build:rollup",
    "start": "cross-env NODE_PATH='source' node devServer.js",
    "debug": "cross-env NODE_PATH='source' echo 'Nothing? `yarn add -g iron-node`' && iron-node source/debug.js",
    "lint": "eslint source",
    "pretest": "yarn run lint",
    "test": "cross-env NODE_PATH='source' babel-node source/test/index.js",
    "watch": "watch 'clear && yarn run test -s' source",
    "update": "updtr"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gaearon/react-transform-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "webpack",
    "babel",
    "react-transform"
  ],
  "author": "Dan Abramov <dan.abramov@me.com> (http://github.com/gaearon)",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/gaearon/react-transform-boilerplate/issues"
  },
  "homepage": "https://github.com/gaearon/react-transform-boilerplate",
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-eslint": "7.1.0",
    "babel-plugin-external-helpers": "6.18.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "cheerio": "0.22.0",
    "cross-env": "3.1.3",
    "deep-freeze": "0.0.1",
    "eslint": "3.9.1",
    "eslint-plugin-react": "6.5.0",
    "ignore-styles": "5.0.1",
    "node-sass": "3.11.1",
    "react-addons-test-utils": "15.3.2",
    "redbox-react": "1.3.2",
    "rimraf": "2.5.4",
    "rollup": "0.36.3",
    "rollup-plugin-babel": "2.6.1",
    "rollup-plugin-commonjs": "5.0.5",
    "rollup-plugin-node-resolve": "2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-sass": "^0.4.4",
    "rollup-plugin-uglify": "1.0.1",
    "tape": "4.6.2",
    "updtr": "0.2.2",
    "watch": "1.0.1"
  },
  "dependencies": {
    "express": "4.14.0",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "redux": "3.6.0"
  }
}
